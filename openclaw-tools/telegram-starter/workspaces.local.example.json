{
  "_comment": "Copy this file to workspaces.local.json and replace the token hash.",
  "workspaces": {
    "demo_workspace": {
      "token_sha256": "REPLACE_WITH_SHA256_OF_WORKSPACE_TOKEN",
      "allow_network": false,
      "permissions": {
        "telegram_send_message": {
          "allowed_chat_ids": [
            "123456789"
          ],
          "max_text_length": 500
        }
      },
      "telegram": {
        "bot_token_env": "OPENCLAW_TELEGRAM_BOT_TOKEN"
      }
    }
  }
}
